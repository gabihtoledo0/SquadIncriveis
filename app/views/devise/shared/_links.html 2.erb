<%- if controller_name != 'sessions' %>
<%= link_to "Entrar", new_session_path(resource_name), :class => "link"%><br />

<% end %>

<div class="container bg-white container-login">
<div class="col-sm field">
<%- if devise_mapping.registerable? && controller_name != 'Registrar' %>
<%= link_to "Primeiro acesso", new_registration_path(resource_name), :class => "link"%>
<% end %>
</div>

  <div class="col-sm field">
    <%- if devise_mapping.recoverable? && controller_name != 'Senha' && controller_name != 'Registrar' %>
    <%= link_to "Esqueci minha senha", new_password_path(resource_name), :class => "link"  %>
    <% end %>
  </div>
  <br>
  
<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
<%= link_to "Não recebeu instruções de confirmação?", new_confirmation_path(resource_name) %><br />
<% end %>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
<%= link_to "Não recebeu instruções de desbloqueio?", new_unlock_path(resource_name) %><br />
<% end %>

<%- if devise_mapping.omniauthable? %>
<%- resource_class.omniauth_providers.each do |provider| %>
<%= link_to "Entrar com #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), :class => "link"  %><br />
<% end %>
<% end %>